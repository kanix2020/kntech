<div class="container mt-3">
  <div class="d-flex justify-content-center">
    <p class="h1">お問い合わせ</p>
  </div>
  <div class="row">
    <div class="col-md-12">
      <form action="https://api.staticforms.xyz/submit" method="post">
        <input type="hidden" name="accessKey" value="アクセスキー">
        <input type="hidden" name="subject" value="件名">
        <input type="hidden" name="redirectTo" value="絶対パス">
        <div class="form-group">
          <label class="col-sm-4 col-form-label">会社名</label>
          <div class="col-sm-12">
            <input type="text" name="name" class="form-control" id="inputName">
          </div>
        </div>
        <div class="form-group">
          <label class="col-sm-4 col-form-label">お名前</label>
          <div class="col-sm-12">
            <input type="text" name="name" class="form-control" id="inputName">
          </div>
        </div>
        <div class="form-group">
          <label for="inputEmail" class="col-sm-4 col-form-label">メールアドレス</label>
          <div class="col-sm-12">
            <input type="email" name="email" class="form-control" id="inputEmail">
          </div>
        </div>
        <div class="form-group">
          <label for="inputEmail" class="col-sm-4 col-form-label">お電話</label>
          <div class="col-sm-12">
            <input type="tel" name="email" class="form-control" id="inputEmail">
          </div>
        </div>
        <div class="form-group">
          <label for="inputTextarea" class="col-sm-4 col-form-label">お問い合わせ内容</label>
          <div class="col-sm-12">
            <textarea name="message" rows="3" class="form-control" id="inputTextarea"></textarea>
          </div>
        </div>
        <div class="text-center">
          <button type="submit" class="btn btn-primary mb-2">送信</button>
        </div>
      </form>
    </div>
  </div>
</div>